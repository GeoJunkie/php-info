{
    "name": "phpinfo",
    "version": "1.0.0",
    "description": "PHP Info Page",
    "author": "Mike Straw",
    "license": "GPL-2.0-or-later",
    "keywords": [
      "WordPress",
      "Theme",
      "Retro"
    ],
    "homepage": "https://github.com/geojunkie/php-info#readme",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/geojunkie/php-info.git"
    },
    "bugs": {
      "web": "https://github.com/geojunkie/php-info/issues"
    },
    "dependencies": {},
    "devDependencies": {
      "@wordpress/browserslist-config": "^2.6.0",
      "@wordpress/scripts": "^5.1.0",
      "autoprefixer": "^9.7.0",
      "postcss-cli": "^6.1.3",
      "rtlcss": "^2.4.0",
      "stylelint": "^11.1.1",
      "stylelint-config-wordpress": "^14.0.0"
    },
    "browserslist": [
      "extends @wordpress/browserslist-config"
    ],
    "scripts": {
      "build": "npm run build:vendor-prefixes && npm run build:rtl && stylelint style.css --fix && stylelint rtl.css --fix",
      "build:rtl": "rtlcss style.css rtl.css",
      "build:vendor-prefixes": "postcss -r --no-map style.css",
      "lint:css": "wp-scripts lint-style 'style.css' 'assets/**/*.css'",
      "lint:pkg-json": "wp-scripts lint-pkg-json"
    }
  }